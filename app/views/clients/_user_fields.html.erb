<table>
<tr class='nested-fields m-b-10'>
  <td class=" p-r-5"><%= f.email_field :email, class: 'form-control mb10', placeholder: "Email" %></td>
  <td class=" p-r-5"><%#= f.text_field :first_name, class: 'form-control', placeholder: "First Name" %></td>
  <td class=" p-r-5"><%#= f.text_field :last_name, class: 'form-control', placeholder: "Last Name" %></td>
  <td class=" p-r-5"><%#= f.hidden_field :_destroy %></td>
  <td>
    <%= link_to_remove_association f do %>
      <i class="fa fa-trash-o" aria-hidden="true"></i>
    <% end %>
  </td>
</tr>
</table>
