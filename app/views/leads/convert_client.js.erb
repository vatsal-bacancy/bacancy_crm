<% if @lead.errors.any? %>
  $("#success_message").html("<%= escape_javascript(render partial: 'layouts/flash') %>");
<% else %>
  $("#modalContainer").html("<%= escape_javascript(render partial: 'convert_client', locals: {lead: @lead, users: @users, client: @client, klass: @klass }) %>")
  $("#CleintModal").modal('show');
<% end %>
