<div class="modal fade" id="importModal" role="dialog">
  <div class="modal-dialog">

    <!-- Modal content-->
    <%= form_with scope: :lead , url: import_file_path do |f| %>

      <div class="modal-content">
        <div class="modal-header">
          <button type="button" class="close" data-dismiss="modal">&times;</button>
          <h4 class="modal-title">Leads</h4>
          <div id='flash_message'></div>
        </div>
        <div class="modal-body">
        <div class='row'>
          <div class='col-md-6'>
            <div class="form-group">
              <%= f.label :select_file %>
              <%= f.file_field :import, class: 'form-control', accept: 'text/csv' %>
            </div>
            <div class="form-group">
            </div>
          </div>
          <div class='col-md-6'>
            <div class="root">
              <%= link_to "Sample CSV", root_url + "/import-example.csv" , class: 'm-t-20 common-link' %>
            </div>
          </div>
        </div>
        <div class="modal-footer">
          <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
          <%= f.submit "CSV Import", class: "btn btn-success" %>

        </div>
      </div>
    <%end%>
  </div>
</div>
