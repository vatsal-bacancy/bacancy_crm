<% if @error %>
  $("#flash_message").html("<%= escape_javascript(render partial: 'layouts/flash') %>");
<% else %>
  $("#success_message").html("<%= escape_javascript(render partial: 'layouts/flash') %>");
  $("#lead-list").html("<%= escape_javascript(render partial: 'lead_list', locals: {leads: @leads, fields: @fields, data: @data}) %>")
  $('#myModal').modal('hide');
<% end %>

